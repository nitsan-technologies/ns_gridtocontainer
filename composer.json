{
    "name": "nitsan/ns-gridtocontainer",
    "type": "typo3-cms-extension",
    "description": "This Extension is use forse who want to switch from EXT:gridelements to EXT:container.",
    "authors": [],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "NITSAN\\NsGridtocontainer\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NITSAN\\NsGridtocontainer\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/ns-gridtocontainer": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "ns_gridtocontainer"
        }
    }
}